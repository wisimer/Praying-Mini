{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": true,
		"create": true,
		"update": "doc.user_id == auth.uid",
		"delete": "doc.user_id == auth.uid"
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"user_id": {
			"bsonType": "string",
			"description": "用户ID",
			"foreignKey": "uni-id-users._id"
		},
		"challenge_id": {
			"bsonType": "string",
			"description": "挑战id",
			"foreignKey": "app-challenge._id"
		},
		"create_date": {
			"bsonType": "timestamp",
			"description": "记录时间",
			"forceDefaultValue": {
				"$env": "now"
			}
		}
	}
}