{
	"bsonType": "object",
	"required": [],
	"permission": {
		"read": true,
		"create": "auth.uid != null",
		"update": "doc.user_id == auth.uid",
		"delete": "doc.user_id == auth.uid"
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"user_id": {
			"bsonType": "string",
			"description": "签到的用户ID",
			"foreignKey": "uni-id-users._id",
			"defaultValue": {
				"$env": "uid"
			}
		},
		"check_date": {
			"bsonType": "string",
			"description": "签到日期"
		},
		"create_date": {
			"bsonType": "timestamp",
			"description": "记录时间",
			"forceDefaultValue": {
				"$env": "now"
			}
		}
	}
}